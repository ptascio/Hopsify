<h1>HI</h1>

<h1>Please fill up the form</h1>
<%= form_for Music.new, :url => {:action => 'index'} do |f| %>
  <p>
    Artist<br/><%= f.text_field 'artist' %>
  </p>
  <p>
    Album<br/><%= f.text_field 'album' %>
  </p>
  <p>
    Track<br/><%= f.text_field 'track' %>
  </p>

  <p>
    <%= f.submit "Submit Data" %>
  </p>
<% end %>
